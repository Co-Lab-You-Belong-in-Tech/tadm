{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["./assets/*"],
      "@components/*": ["./components/*"],
      "@config/*": ["./config/*"],
      "@contexts/*": ["./contexts/*"],
      "@hooks/*": ["./hooks/*"],
      "@utils/*": ["./utils/*"],
      "@screens/*":["./screens/*"],
    }
  },
  "include": ["@assets", "@components", "@config", "@contexts", "@hooks", "@utils", "@screens"],
  "exclude": ["node_modules"]
}